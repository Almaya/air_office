<%= simple_form_for @workspace do |w| %>
  <%= w.input :name %>
  <%= w.input :address,
  input_html: {data: {address_autocomplete_target: "address"}},
  wrapper_html: {data: {controller: "address-autocomplete", address_autocomplete_api_key_value: ENV["MAPBOX_API_KEY"]}}
%>
  <%= w.input :neighborhood %>
  <%= w.input :price %>
  <%= w.input :photos, as: :file, input_html: { multiple: true } %>
  <%= w.button :submit %>
<% end %>
