<h1>Workspaces</h1>
<p>Here you can find all the workspaces</p>

<div class="container-page">
  <%# <%= render 'shared/card' %>
  <div class="cards">
    <% @workspaces.each do |workspace| %>
      <div class="card">
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/breakfast.jpg)">
        </div>
        <div>
          <%= link_to workspace.name, workspace_path(workspace) %>,
          <%= workspace.neighborhood %>,
          <%= workspace.price %>â‚¬
        </div>
      </div>
    <% end %>
  </div>
</div>